terraform {
  required_providers {
    aws = {
      source  = "hashicorp/aws"
      version = "~> 5.3
    }
  }
}

provider "aws" {
  region = "us-east-1"
}

resource "aws_instance" "example" {
  ami           = "ami-0c02fb55956c7d316" # Amazon Linux 2 AMI (example)
  instance_type = "t2.medium"

  tags = {
    Name = "MyEC2Instance5"
  }
}
